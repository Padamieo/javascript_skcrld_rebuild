<?xml version='1.0' encoding='utf-8'?>
<widget id="uk.co.padam.skcrl" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Super Kitty Cat Rainbow Lazers</name>
    <description>
        A simple constant runner game for mobile devices
    </description>
    <author email="michaeladamlockwood@googlemail.com" href="http://www.padam.co.uk">
        Padam Games
    </author>
    <content src="index.html" />
    <preference name="Fullscreen" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-globalization" source="pgb" spec="1.0.3" />
    <plugin name="cordova-plugin-vibration" source="pgb" spec="2.1.1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="browser" spec="~4.0.0" />
</widget>
